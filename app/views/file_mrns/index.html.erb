<%= flash[:notice] %>
<table>
  <thead>
    <tr>
      <th>File</th>
      <th>MRN</th>
    </tr>
  </thead>
  <tbody>
    <!-- display here all the files and MRNs -->
    <% @fileMRNs.each do |this_one| %>
      <tr>
        <td><%= this_one.file_name %></td>
        <td><%= this_one.mrn %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div>
<h3>Import a QRDA1 zip file</h3>
  <%= form_tag import_file_mrns_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import zip" %>
  <% end %>
</div>